<html>
<head>
  <meta charset='utf-8'>
  <title>Image Justifier</title>
</head>
<body>
  <h2>Here are some fake images</h2><br>
  <% collection.rows.each do |row| %>
  <%   row.pictures.each do |pic| %>
  <a href="<%= pic.src %>"><img src="<%= pic.src %>" width="<%= pic.width %>" height="<%= pic.height %>"></a>
  <%   end %><br>
  <% end %>
</body>
</html>
